
<% if user_signed_in? %>
  <% if current_user.admin? %>

    <h1>Editer rapport DOI</h1>

    <%= render "form", rapportdoi: @rapportdoi %>

    <br>

    <div>
      <%= link_to "", @rapportdoi, class:"fa-solid fa-eye btn btn-primary" %> |
      <%= link_to "", rapportdois_path, class:"fa-solid fa-arrow-rotate-left btn btn-secondary"  %>
    </div>

  <% end %>
<% else %>
  <%= render "shared/demandeconnexion" %>
<% end %>